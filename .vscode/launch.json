{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug @org/clickup-mcp-server with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "@org/clickup-mcp-server"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/apps/clickup-mcp-server/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug @org/digitalocean-mcp-server with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "@org/digitalocean-mcp-server"],
      "env": {
        "NODE_OPTIONS": "--inspect=9230"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/apps/digitalocean-mcp-server/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug @jeff-stapleton/tailscale-mcp-server with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "@jeff-stapleton/tailscale-mcp-server"],
      "env": {
        "NODE_OPTIONS": "--inspect=9231"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/apps/tailscale-mcp-server/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    }
  ]
}
